  "author": "fabien Greard",
  "repository": {
    "type": "git",
    "url": "git://github.com/FabienGreard/gdax-trading.git"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "redux-devtools": "^3.4.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-test-renderer": "^16.3.1",
    "redux-test-utils": "^0.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "server": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --coverage --watchAll --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/keys.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/_services/*.{js,jsx}",
      "!src/_constants/*.{js,jsx}",
      "!src/_reducers/*.{js,jsx}",
      "!src/registerServiceWorker.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/helpers/*.{js,jsx}",
      "!src/**/index.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  },
  "keywords": ["react", "gdax", "real-time", "payments", "trading"]
}
